[user]
  name = Parinda Darden
  email = parinda.darden@gmail.com

[alias]
  br = branch
  hist = log --pretty=format:'%Cgreen%h%Creset %ad | %s%Cred%d%Creset [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
 
[color]
  branch = auto
  diff = auto
  status = auto
 
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
 
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
 
[color "status"]
  added = green
  changed = yellow
  untracked = cyan
  deleted = red bold
 
[core]
  excludesfile = ~/.gitignore
  editor = mvim -f
[credential]
  helper = store
[push]
  default = simple
